{
  "name": "vue2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --mode express",
    "dev:laravel": "vue-cli-service serve --mode laravel",
    "build": "vue-cli-service build",
    "build-express": "vue-cli-service build --mode express",
    "build-laravel": "vue-cli-service build --mode laravel",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@vue/composition-api": "^1.7.1",
    "axios": "0.18.1",
    "core-js": "^3.8.3",
    "element-ui": "^2.15.13",
    "normalize.css": "7.0.0",
    "vue": "2.6.10",
    "vue-router": "^3.0.6",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "sass": "1.26.8",
    "sass-loader": "8.0.2",
    "unplugin-vue2-script-setup": "^0.11.3",
    "vue-template-compiler": "2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "requireConfigFile": false
    },
    "rules": {
      "vue/multi-word-component-names": "off",
      "vue/no-use-v-if-with-v-for": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
